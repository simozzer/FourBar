1 DOKE #9C,#1FFF
10 CALL #2001: CLS: DIM N$: P=PEEK(#8FFF)
20 IF P=1 THEN INPUT "SAVE NAME";N$: CSAVE N$,A#9000,E#9300: GOTO 10
30 IF P=2 THEN INPUT "LOAD NAME";N$: CLOAD N$:CALL #291B: GOTO 10
40 IF P=3 THEN GOTO 300
50 PRINT "GOODBYE": END
300 PRINT "PRINTING..."
305 DIM A$
310 FOR N=#9000 TO #9300 STEP 8 
320 A$="BYT " + HEX$(PEEK(N)) + ","
330 A$=A$+HEX$(PEEK(N+1)) + ","
340 A$=A$+HEX$(PEEK(N+2)) + ","
350 A$=A$+HEX$(PEEK(N+3)) + ","
360 A$=A$+HEX$(PEEK(N+4)) + ","
370 A$=A$+HEX$(PEEK(N+5)) + ","
380 A$=A$+HEX$(PEEK(N+6)) + ","
390 A$=A$+HEX$(PEEK(N+7))
400 LPRINT A$
410 NEXT N
420 PRINT "JOB COMPLETE: GOODBYE"